{
  "name": "vue-web-component",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -c bgBlue,bgMagenta -n vite,http \"npm run build:watch\" \"npm run serve\"",
    "build:watch": "vite build --watch",
    "serve": "http-server ./dist -p3001 -c-1 --cors",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1",
    "sass": "^1.77.6",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.21"
  }
}
